{
    "openapi": "3.0.0",
    "info": {
        "title": "API Documentation",
        "description": "API Usuários",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        }
    ],
    "paths": {
        "/api/usuarios": {
            "get": {
                "tags": [
                    "Usuários"
                ],
                "summary": "Listar todos os usuários",
                "description": "Retorna a lista de usuários",
                "operationId": "600ec787a82d08ec298424fd97dd33d9",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "api/usuario/{cpf}": {
            "get": {
                "tags": [
                    "Usuários"
                ],
                "summary": "Buscar um usuário pelo CPF",
                "operationId": "a1d534d525c2b61beeadab9881321e04",
                "parameters": [
                    {
                        "name": "cpf",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usuário encontrado"
                    },
                    "404": {
                        "description": "Usuário não encontrado"
                    }
                }
            }
        },
        "api/usuario": {
            "post": {
                "tags": [
                    "Usuários"
                ],
                "summary": "Cadastrar um novo usuário",
                "operationId": "e15ed227f3197435ee2348cff707e2db",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "cpf",
                                    "nome",
                                    "data_nascimento"
                                ],
                                "properties": {
                                    "cpf": {
                                        "type": "integer"
                                    },
                                    "nome": {
                                        "type": "string"
                                    },
                                    "data_nascimento": {
                                        "type": "string",
                                        "format": "date"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Usuário cadastrado com sucesso"
                    },
                    "409": {
                        "description": "Não foi possível cadastrar o usuário"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Usuários",
            "description": "Usuários"
        }
    ]
}